{
  "productName": "TheDesk",
  "appId": "next.top.thedesk",
  "asarUnpack": [
    "native/**"
  ],
  "files": [
    "main-build",
    "renderer/out",
    "native"
  ],
  "mas": {
    "type": "distribution",
    "entitlements": "build-tool/parent.plist",
    "entitlementsInherit": "build-tool/child.plist",
    "entitlementsLoginHelper": "build-tool/loginhelper.plist",
    "hardenedRuntime": false,
    "gatekeeperAssess": false,
    "extendInfo": {
      "ITSAppUsesNonExemptEncryption": "false"
    },
    "provisioningProfile": "build-tool/thedesk_MAS.provisionprofile",
    "minimumSystemVersion": "14.0"
  },
  "mac": {
    "icon": "assets/icon.icns",
    "target": [
      "mas"
    ],
    "hardenedRuntime": true,
    "gatekeeperAssess": false,
    "notarize": false,
    "category": "public.app-category.social-networking"
  }
}
